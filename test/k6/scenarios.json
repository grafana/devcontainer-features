{
  "k6-with-version": {
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
    "features": {
      "k6": {
        "version": "1.0.0"
      }
    }
  },
  "k6-with-extensions": {
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
    "features": {
      "k6": {
        "with": ["github.com/grafana/xk6-example", "github.com/grafana/xk6-output-example"]
      }
    }
  },
  "k6-with-version-and-extensions": {
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
    "features": {
      "k6": {
        "version": "1.0.0",
        "with": ["github.com/grafana/xk6-example", "github.com/grafana/xk6-output-example"]
      }
    }
  },
  "k6-with-extensions-has-go": {
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
    "features": {
      "k6": {
        "with": ["github.com/grafana/xk6-example", "github.com/grafana/xk6-output-example"]
      },
      "ghcr.io/devcontainers/features/go:1": {}
    }
  }
}
